{
    "compilerOptions": {
        "lib": ["es2021"],
        "module": "ES2022",
        "target": "es2021",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
    },
    "include": ["src"],
    "exclude": ["node_modules"],
}